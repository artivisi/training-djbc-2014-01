<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HaloServlet</servlet-name>
        <servlet-class>id.go.customs.training.gudang.web.HaloServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BarangListServlet</servlet-name>
        <servlet-class>id.go.customs.training.gudang.web.BarangListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BarangFormServlet</servlet-name>
        <servlet-class>id.go.customs.training.gudang.web.BarangFormServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BarangUploadServlet</servlet-name>
        <servlet-class>id.go.customs.training.gudang.web.BarangUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VendorListServlet</servlet-name>
        <servlet-class>id.go.customs.training.gudang.web.VendorListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GedungListServlet</servlet-name>
        <servlet-class>id.go.customs.training.gudang.web.GedungListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HaloServlet</servlet-name>
        <url-pattern>/halo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BarangListServlet</servlet-name>
        <url-pattern>/barang/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BarangFormServlet</servlet-name>
        <url-pattern>/barang/form</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BarangUploadServlet</servlet-name>
        <url-pattern>/barang/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VendorListServlet</servlet-name>
        <url-pattern>/VendorListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GedungListServlet</servlet-name>
        <url-pattern>/gedung/list</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
